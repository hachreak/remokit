{
    "prepare_batch": "remokit.experiments.rgb_cnn.prepare_batch",
    "kfolds": 10,
    "full_image_size": {
      "img_x": 300,
      "img_y": 300
    },
    "image_size": {
      "img_x": 150,
      "img_y": 150
    },
    "model": "remokit.models.model08.get_model",
    "batch_size": 14,
    "epochs": 150,
    "repeat_seeds": 10,
    "metrics": "data/metrics/face_metrics_model08.json",
    "verbose": 2,
    "directory": "data/stage/01_face_model08",
    "get_label": "remokit.preprocessing.extract_face.get_label",
    "best_model": "data/best/01_face_model08.h5",
    "preprocess": [
      {
        "prepare_batch": "remokit.preprocessing.extract_face.prepare_batch",
        "save": "remokit.preprocessing.extract_face.save",
        "has_faces": true,
        "get_label": "remokit.datasets.kdef.get_label",
        "get_files": "remokit.datasets.kdef.get_files",
        "directory": "data/KDEF-straight/"
      }
    ],
    "training": {
      "early_stop": {
        "monitor": "val_loss",
        "min_delta": 0,
        "patience": 20
      },
      "reduce_lr": {
        "mode": "auto",
        "monitor": "val_loss",
        "factor": 0.5,
        "patience": 10,
        "min_lr": 0.00001
      },
      "optimizer": {
        "lr": 0.001
      }
    }
}
